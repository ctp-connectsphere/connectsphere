{
  "name": "connectsphere",
  "version": "1.0.0",
  "description": "**Connecting Students, One Study Session at a Time**",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:debug": "NODE_OPTIONS='--inspect' next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf .next out dist node_modules/.cache",
    "clean:all": "rm -rf .next out dist node_modules/.cache .turbo",
    "analyze": "ANALYZE=true npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "tsx scripts/prisma-studio.ts",
    "db:studio:basic": "prisma studio",
    "db:manager": "tsx scripts/db-manager.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:dev": "tsx scripts/seed-dev.ts",
    "db:seed:comprehensive": "tsx scripts/seed-comprehensive.ts",
    "db:reset": "prisma migrate reset",
    "db:reset:dev": "prisma migrate reset --skip-seed && npm run db:seed:dev",
    "validate:config": "tsx scripts/validate-config.ts",
    "debug:db": "tsx scripts/debug-db.ts",
    "debug:redis": "tsx scripts/debug-redis.ts",
    "debug:all": "npm run debug:db && npm run debug:redis",
    "redis:browse": "tsx scripts/browse-redis.ts",
    "redis:test": "tsx scripts/test-redis-data.ts",
    "test": "echo \"No tests specified\" && exit 0",
    "test:watch": "echo \"No tests specified\" && exit 0",
    "test:coverage": "echo \"No tests specified\" && exit 0",
    "prepare": "husky install || true",
    "pre-commit": "npm run lint:staged && npm run type-check",
    "pre-push": "npm run lint && npm run type-check && npm run build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.0",
    "@prisma/client": "^6.17.1",
    "@upstash/redis": "^1.35.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "next": "^15.5.4",
    "next-auth": "^5.0.0-beta.29",
    "prisma": "^6.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "campus",
    "study",
    "matching"
  ],
  "author": "",
  "license": "ISC",
  "type": "module"
}
